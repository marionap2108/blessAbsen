{
  "version": 2,
  "routes": [
    {
      "src": "/firebase-messaging-sw.js",
      "dest": "/firebase-messaging-sw.js",
      "headers": {
        "cache-control": "no-cache",
        "Service-Worker-Allowed": "/",
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
} 